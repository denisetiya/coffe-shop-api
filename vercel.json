{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "src/**/*.ts",
            "tsconfig.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/main.ts"
      }
    ]
  }
  